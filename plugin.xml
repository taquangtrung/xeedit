<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <category
            name="Xedipse Command"
            id="Xedipse.commands.category">
      </category>
      <command
            categoryId="Xedipse.commands.category"
            id="Xedipse.commands.moveCursorUpwardByIndent"
            name="Move Cursor Upward By Indent">
      </command>
      <command
            categoryId="Xedipse.commands.category"
            id="Xedipse.commands.moveCursorDownwardByIndent"
            name="Move Cursor Downward By Indent">
      </command>
      <command
            categoryId="Xedipse.commands.category"
            id="Xedipse.commands.moveCursorUpwardByBlock"
            name="Move Cursor Upward By Block">
      </command>
      <command
            categoryId="Xedipse.commands.category"
            id="Xedipse.commands.moveCursorDownwardByBlock"
            name="Move Cursor Downward By Block">
      </command>
      <command
            categoryId="Xedipse.commands.category"
            id="Xedipse.commands.selectTextUpwardByIndent"
            name="Select Text Upward By Indent">
      </command>
      <command
            categoryId="Xedipse.commands.category"
            id="Xedipse.commands.selectTextDownwardByIndent"
            name="Select Text Downward By Indent">
      </command>
      <command
            categoryId="Xedipse.commands.category"
            id="Xedipse.commands.selectTextUpwardByBlock"
            name="Select Text Upward By Block">
      </command>
      <command
            categoryId="Xedipse.commands.category"
            id="Xedipse.commands.selectTextDownwardByBlock"
            name="Select Text Downward By Block">
      </command>
      <command
            categoryId="Xedipse.commands.category"
            id="Xedipse.commands.removeTrailingWhiteSpace"
            name="Remove Trailing White Space">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="xedipse.handlers.MoveCursorUpwardByIndent"
            commandId="Xedipse.commands.moveCursorUpwardByIndent">
      </handler>
      <handler
            class="xedipse.handlers.MoveCursorDownwardByIndent"
            commandId="Xedipse.commands.moveCursorDownwardByIndent">
      </handler>
      <handler
            class="xedipse.handlers.MoveCursorUpwardByBlock"
            commandId="Xedipse.commands.moveCursorUpwardByBlock">
      </handler>
      <handler
            class="xedipse.handlers.MoveCursorDownwardByBlock"
            commandId="Xedipse.commands.moveCursorDownwardByBlock">
      </handler>
      <handler
            class="xedipse.handlers.SelectTextUpwardByIndent"
            commandId="Xedipse.commands.selectTextUpwardByIndent">
      </handler>
      <handler
            class="xedipse.handlers.SelectTextDownwardByIndent"
            commandId="Xedipse.commands.selectTextDownwardByIndent">
      </handler>
      <handler
            class="xedipse.handlers.SelectTextUpwardByBlock"
            commandId="Xedipse.commands.selectTextUpwardByBlock">
      </handler>
      <handler
            class="xedipse.handlers.SelectTextDownwardByBlock"
            commandId="Xedipse.commands.selectTextDownwardByBlock">
      </handler>
      <handler
            class="xedipse.handlers.RemoveTrailingWhiteSpace"
            commandId="Xedipse.commands.removeTrailingWhiteSpace">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="Xedipse.commands.moveCursorUpwardByIndent"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+ARROW_UP">
      </key>
      <key
            commandId="Xedipse.commands.moveCursorDownwardByIndent"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+ARROW_DOWN">
      </key>
      <key
            commandId="Xedipse.commands.moveCursorUpwardByBlock"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+[">
      </key>
      <key
            commandId="Xedipse.commands.moveCursorDownwardByBlock"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+]">
      </key>
      <key
            commandId="Xedipse.commands.selectTextUpwardByIndent"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M2+ARROW_UP">
      </key>
      <key
            commandId="Xedipse.commands.selectTextDownwardByIndent"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M2+ARROW_DOWN">
      </key>
      <key
            commandId="Xedipse.commands.selectTextUpwardByBlock"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M2+[">
      </key>
      <key
            commandId="Xedipse.commands.selectTextDownwardByBlock"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M2+]">
      </key>
      <key
            commandId="Xedipse.commands.removeTrailingWhiteSpace"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+\">
      </key>
   </extension>
